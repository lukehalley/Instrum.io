<section class="content">

    <link rel="stylesheet" type="text/css" href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css">

        <div class="row">
            <div class="col-lg-12">
                <div class="main-box no-header clearfix">
                    <div class="main-box-body clearfix">
                        <div class="table-responsive">
                            <table class="table user-list">
                                <!--TITLES-->
                                    <thead>
                                    <tr>
                                        <th style="text-align:center"><span></span></th>
                                        <th style="text-align:center"><span>Location</span></th>
                                        <th style="text-align:center"><span>Email</span></th>
                                        <th style="text-align:center">Actions &nbsp;</th>
                                    </tr>
                                    </thead>
                                <!--CONTENT-->
                                <div ng-controller="usersController as list">
                                    <tbody ng-repeat="user in users">
                                        <tr>
                                            <td style="text-align:center">
                                                <img src="https://bootdey.com/img/Content/user_1.jpg" alt="">
                                                <a href="#" class="user-link">{{user.name}}</a>
                                                Username: {{user.username}}
                                            </td>
                                            <td style="text-align:center">{{user.location}}</td>
                                            <td style="text-align:center">
                                                <a href="#">{{user.email}}</a>
                                            </td>
                                            <td style="text-align:center">
                                                <button type="button" class="btn bg-red waves-effect">
                                                    <i href="javascript:void(0);" class="material-icons">remove_red_eye</i>
                                                    <span>View Uploaded Beats</span>
                                                </button>
                                            </td>
                                        </tr>
                                </tbody>
                                </div>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <!--<p>{{ message }}</p>-->
</section>